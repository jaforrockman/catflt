<p id="notice"><%= notice %></p>

<h1>Vouchers</h1>

<table class="table table-bordered">
  <thead>
    <tr>
    <th>iv</th>
     <th>Catering</th>
      <th>Name</th>
      <th>Mess</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vouchers.each do |voucher| %>
      <tr>
      <td><%= voucher.iv %></td>
        <td><%= Catering.find(voucher.catering_id).name %></td>
        <td><%= voucher.name %></td>
        <td><%= Mess.find(voucher.mess_id).name %></td>
        <td><%= link_to 'Show', voucher %></td>
        <td><%= link_to 'Edit', edit_voucher_path(voucher) %></td>
        <td><%= link_to 'Destroy', voucher, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Voucher', new_voucher_path %>
